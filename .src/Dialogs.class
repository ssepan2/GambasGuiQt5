Public Function FormatAbout() As String

    Dim sResult As String = Null

    sResult &= Subst("&1\r\n&2\r\n&3\r\n&4\r\n&5\r\n", Application.Name, Application.Title, Application.Version, Application.Id, Application.Path)
    Return sResult
Catch 'ex As Exception
    Debug Log.FormatError() 'Error.Text 'TODO:get this dump into a log file
    Debug Error.Text
    Return ""

End
