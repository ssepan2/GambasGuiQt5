# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,64,64)
  Expand = True
  Icon = Picture["icon:/32/gnu"]
  AutoResize = True
  Padding = 8
  { MenuFile Menu
    Text = Shortcut(("File"), "F")
    { MenuFileNew Menu
      Text = Shortcut(("New"), "N")
    }
    { MenuFileOpen Menu
      Text = Shortcut(("Open"), "O") & "..."
    }
    { MenuFileSave Menu
      Text = Shortcut(("Save"), "S")
    }
    { MenuFileSaveAs Menu
      Text = Shortcut(("Save As"), "a") & "..."
    }
    { Menu1 Menu
    }
    { MenuFileExit Menu
      Text = Shortcut(("Exit"), "x")
    }
  }
  { MenuHelp Menu
    Text = Shortcut(("Help"), "H")
    { MenuHelpAbout Menu
      Text = Shortcut(("About"), "A")
    }
  }
  { ToolBar1 ToolBar
    MoveScaled(1,0,62,4)
    Expand = True
    Separator = True
    { ToolButtonFileNew ToolButton
      MoveScaled(0,0,4,4)
      ToolTip = ("New")
      AutoResize = True
      Picture = Picture["icon:/22/new"]
    }
    { ToolButtonFileOpen ToolButton
      MoveScaled(3,0,4,4)
      ToolTip = ("Open")
      AutoResize = True
      Picture = Picture["icon:/22/open"]
    }
    { ToolButtonFileSave ToolButton
      MoveScaled(6,0,4,4)
      ToolTip = ("Save")
      AutoResize = True
      Picture = Picture["icon:/22/save"]
    }
    { ToolButtonFileSaveAs ToolButton
      MoveScaled(9,0,4,4)
      ToolTip = ("Save As") & "..."
      AutoResize = True
      Picture = Picture["icon:/22/save-as"]
    }
  }
  { Panel1 Panel
    MoveScaled(1,55,62,5)
    Expand = True
    AutoResize = True
    Border = Border.Sunken
    { StatusLabel Label
      MoveScaled(1,1,31,3)
    }
    { StatusPictureBox PictureBox
      MoveScaled(58,1,4,3)
      AutoResize = True
    }
    { StatusProgressBar ProgressBar
      MoveScaled(33,1,24,3)
    }
  }
  { Panel2 Panel
    MoveScaled(2,5,60,14)
    { SomeStringLabel Label
      MoveScaled(0,0,12,4)
      Text = ("Some String:")
      Alignment = Align.Right
    }
    { SomeStringTextBox TextBox
      MoveScaled(13,0,24,4)
    }
    { SomeIntegerLabel Label
      MoveScaled(0,5,12,4)
      Text = ("Some Integer:")
      Alignment = Align.Right
    }
    { SomeIntegerValueBox ValueBox
      MoveScaled(13,5,24,4)
      Alignment = Align.Right
    }
    { SomeBooleanLabel Label
      MoveScaled(0,10,12,4)
      Text = ("Some Boolean:")
      Alignment = Align.Right
    }
    { SomeBooleanCheckBox CheckBox
      MoveScaled(13,10,3,4)
    }
  }
  { HBox1 HBox
    MoveScaled(2,49,60,4)
    Expand = True
    AutoResize = True
    { Spring1 Spring
      MoveScaled(10,1,4,3)
    }
    { ButtonClose Button
      MoveScaled(44,0,16,4)
      Text = ("Close")
    }
  }
}
